{
  "name": "axi-peaq-integration",
  "version": "0.0.1",
  "description": "AXI Mobility blockchain integration with Peaq Network",
  "author": "AXI Development Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "test:cov": "jest --config jest.config.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.config.js",
    "test:unit": "jest --config jest.config.js --selectProjects unit --verbose",
    "test:unit:watch": "jest --config jest.config.js --selectProjects unit --watch",
    "test:unit:blockchain": "jest --config jest.config.js test/unit/blockchain --verbose",
    "test:unit:telemetry": "jest --config jest.config.js test/unit/telemetry --verbose",
    "test:integration": "jest --config jest.config.js --selectProjects integration --verbose --runInBand",
    "test:integration:blockchain": "jest --config jest.config.js test/integration/blockchain --verbose --runInBand",
    "test:e2e": "jest --config test/jest-e2e.json --runInBand",
    "test:e2e:watch": "jest --config test/jest-e2e.json --watch --runInBand",
    "test:contracts": "hardhat test",
    "test:contracts:coverage": "hardhat coverage",
    "test:contracts:gas": "REPORT_GAS=true hardhat test",
    "test:tranche1": "npm run test:unit && npm run test:integration && npm run test:contracts",
    "test:tranche1:full": "npm run test:tranche1 && npm run test:e2e",
    "test:setup": "cp .env.example .env.test",
    "test:clean": "rm -rf coverage/ .nyc_output/ test-results/",
    "migration:generate": "typeorm migration:generate -d src/database/data-source.ts",
    "migration:run": "typeorm migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm migration:revert -d src/database/data-source.ts",
    "schema:drop": "typeorm schema:drop -d src/database/data-source.ts",
    "schema:sync": "typeorm schema:sync -d src/database/data-source.ts",
    "seed:run": "ts-node src/database/seeds/run-seeds.ts",
    "db:reset": "yarn schema:drop && yarn migration:run && yarn seed:run",
    "blockchain:compile": "hardhat compile",
    "blockchain:test": "hardhat test",
    "blockchain:deploy:local": "hardhat run scripts/deploy.ts --network localhost",
    "blockchain:deploy:testnet": "hardhat run scripts/deploy.ts --network krest",
    "docs:generate": "compodoc -p tsconfig.json -s",
    "docs:serve": "compodoc -p tsconfig.json -s --watch",
    "prepare": "husky",
    "commit": "git-cz",
    "release": "standard-version",
    "docker:build": "docker build -t axi-peaq-integration .",
    "docker:run": "docker run -p 5000:5000 axi-peaq-integration",
    "typecheck": "tsc --noEmit",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.8.0",
    "@ethersproject/contracts": "^5.8.0",
    "@liaoliaots/nestjs-redis": "^10.0.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@peaq-network/sdk": "^0.9.2",
    "@polkadot/api": "^16.1.1",
    "@polkadot/keyring": "^13.5.1",
    "@polkadot/util": "^13.5.1",
    "@polkadot/util-crypto": "^13.5.1",
    "axios": "^1.9.0",
    "bcrypt": "^6.0.0",
    "bignumber.js": "^9.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto-js": "^4.2.0",
    "ethers": "^6.11.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.16.0",
    "redis": "^5.5.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.2",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.8",
    "@nomicfoundation/hardhat-ignition": "^0.15.11",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^5.0.0",
    "@nomicfoundation/hardhat-verify": "^2.0.0",
    "@nomicfoundation/ignition-core": "^0.15.11",
    "@openzeppelin/contracts": "^5.3.0",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.17.17",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.4",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "chai": "^4.2.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "hardhat": "^2.24.1",
    "hardhat-gas-reporter": "^1.0.10",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "nock": "^14.0.5",
    "prettier": "^3.4.2",
    "solidity-coverage": "^0.8.12",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/$1",
      "^@/blockchain/(.*)$": "<rootDir>/blockchain/$1",
      "^@/telemetry/(.*)$": "<rootDir>/telemetry/$1",
      "^@/common/(.*)$": "<rootDir>/common/$1",
      "^@/config/(.*)$": "<rootDir>/config/$1",
      "^@/database/(.*)$": "<rootDir>/database/$1",
      "^@/api/(.*)$": "<rootDir>/api/$1"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../test/setup.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}